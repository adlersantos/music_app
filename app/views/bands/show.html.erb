<h1><%= @band.name %></h1>

<h3>Albums and Tracks</h3>

<ul>
  <% @band.albums.each do |album| %>
    <li>
    <%= link_to "#{album.title}", album_url(album) %> <br />
      <ul>
        <% album.tracks.each do |track| %>
          <%= link_to "#{track.name}", album_track_url(album, track) %> <br />
        <% end %>
      </ul>
    </li> <br />
  <% end %>
</ul>